{
    "name": "adal-angular5",
    "version": "2.1.1",
    "description": "ADAL wrapper for Angular5.",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "packagr": "ng-packagr -p ng-package.json",
        "pack": "npm pack ./packagr-dist/",
        "publish": "npm publish ./packagr-dist/ --tag beta",
        "publish-prod": "npm publish ./packagr-dist/",
        "test:once": "ng test --watch=false",
        "ci": "npm run lint && npm run test:once",
        "packagr-publish": "npm version patch && npm run packagr && npm run pack && npm run publish"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/grumar/adal-angular5.git"
    },
    "keywords": [
        "adal",
        "angular5"
    ],
    "author": "Ben Baran",
    "bugs": {
        "url": "https://github.com/grumar/adal-angular5/issues"
    },
    "homepage": "https://github.com/grumar/adal-angular5#readme",
    "private": false,
    "dependencies": {
        "@angular/animations": "^5.0.0",
        "@angular/common": "^5.0.0",
        "@angular/compiler": "^5.0.0",
        "@angular/core": "^5.0.0",
        "@angular/forms": "^5.0.0",
        "@angular/http": "^5.0.0",
        "@angular/platform-browser": "^5.0.0",
        "@angular/platform-browser-dynamic": "^5.0.0",
        "@angular/router": "^5.0.0",
        "adal-angular": "=1.0.15",
        "core-js": "^2.4.1",
        "rxjs": "^5.5.2",
        "zone.js": "^0.8.14"
    },
    "devDependencies": {
        "@angular-devkit/core": "^0.4.6",
        "@angular/cli": "1.6.3",
        "@angular/compiler-cli": "^5.0.0",
        "@angular/language-service": "^5.0.0",
        "@types/jasmine": "^2.8.6",
        "@types/jasminewd2": "~2.0.2",
        "@types/node": "~6.0.60",
        "codelyzer": "^4.0.1",
        "jasmine-core": "~2.6.2",
        "jasmine-spec-reporter": "~4.1.0",
        "karma": "~1.7.0",
        "karma-chrome-launcher": "~2.1.1",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "ng-packagr": "^2.2.0",
        "protractor": "~5.1.2",
        "ts-node": "~3.2.0",
        "tslint": "~5.7.0",
        "typescript": "~2.4.2"
    }
}
